    <record id="view_sale_order_form_toggle_processed" model="ir.ui.view">
        <field name="name">sale.order.form.toggle.processed</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <!-- Lis채t채채n processed-sarake n채kyviin jos puuttuu -->
            <xpath expr="//field[@name='order_line']//list" position="inside">
                <field name="processed" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
